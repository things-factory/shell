    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "graphql-upload": "^8.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "imports-loader": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.6.2",
    "koa-body": "^4.0.4",
