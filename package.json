{
  "name": "@things-factory/shell",
  "version": "0.5.165",
  "description": "things app starter kit",
  "bin": {
    "things-factory": "bin/things-factory",
    "things-factory-dev": "bin/things-factory-dev",
    "things-factory-migration": "bin/things-factory-migration",
    "things-factory-app": "bin/things-factory-app"
  },
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "repository": "http://github.com/things-factory/shell",
  "author": "Hearty Oh <heartyoh@hatiolab.com>",
  "license": "MIT",
  "things-factory": true,
  "scripts": {
    "serve": "env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server.ts",
    "serve:dev": "npm run build:server && env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server-dev.ts",
    "serve:dev:client": "env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server-dev.ts",
    "serve:dev:server": "nodemon --config nodemon-server.json",
    "serve:dev:full": "nodemon --config nodemon-full.json",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm run clean:server && tsc",
    "build:client": "npm run clean:client && webpack-cli --mode production",
    "build:app": "webpack-cli --config ./webpack.config.app.js && ./bin/things-factory-app --platform=android --mode=build",
    "build:app:android": "./bin/things-factory-app --platform=android --mode=build",
    "clean": "npm run clean:server && npm run clean:client",
    "prepublish": "npm run build:server",
    "clean:server": "rm -rf dist-server",
    "clean:client": "rm -rf dist-client",
    "start": "node dist-server/server.js",
    "start:app": "webpack-cli --config ./webpack.config.app.js && ./bin/things-factory-app",
    "start:app:android": "./bin/things-factory-app --platform=android",
    "emulate:app": "./bin/things-factory-app --emulate",
    "emulate:app:android": "./bin/things-factory-app --emulate --platform=android",
    "migration": "npm run build:server && node bin/things-factory-migration",
    "migration:generate": "node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.0",
    "@hatiolab/things-scene": "*",
    "@material/mwc-button": "^0.6.0",
    "@material/mwc-icon": "^0.6.0",
    "@things-factory/env": "*",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.0.10",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "apollo-cache-inmemory": "^1.6.1",
    "apollo-client": "^2.6.1",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-server-koa": "^2.6.1",
    "app-root-path": "^2.2.1",
    "args": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "cordova-android": "^8.0.0",
    "cordova-plugin-add-swift-support": "^2.0.1",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-fetch": "git+https://github.com/aporat/cordova-plugin-fetch.git",
    "cordova-plugin-inappbrowser": "^3.1.0",
    "cordova-plugin-networkinterface": "^2.0.0",
    "cordova-wheel-selector-plugin": "^1.1.3",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "csvtojson": "^2.0.10",
    "file-loader": "^4.0.0",
    "glob": "^7.1.3",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "graphql-upload": "^8.0.2",
    "graphql-voyager": "^1.0.0-rc.27",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "imports-loader": "^0.8.0",
    "koa": "^2.6.2",
    "koa-body": "^4.0.4",
    "koa-bodyparser": "^4.2.0",
    "koa-ejs-remote": "^1.1.6",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa-unless": "^1.0.7",
