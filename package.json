    "regenerator-runtime": "^0.13.2",
    "reselect": "^4.0.0",
    "scrollbooster": "^1.1.0",
    "sqlite3": "^4.0.4",
    "swipe-listener": "^1.1.0",
    "terser": "4.2.1",
    "text-loader": "^0.0.1",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.7.0",
    "typeorm": "0.2.18",
    "typescript": "3.5.1",
    "url-loader": "^2.0.0",
    "web-animations-js": "^2.3.1",
    "web-push": "^3.3.5",
    "webfontloader": "^1.6.28",
    "webpack": "^4.36.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.6.0",
    "webpack-hot-client": "^4.1.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@things-factory/board-test": "*",
    "@things-factory/board-ui": "*",
    "@things-factory/opa-app": "*",
    "@things-factory/opa-test": "*",
    "cordova-plugin-qr-barcode-scanner": "~8.0.3",
    "cordova-plugin-whitelist": "1",
    "cordova-plugin-zeroconf": "~1.4.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-networkinterface": {},
      "cordova-plugin-device": {},
      "cordova-plugin-fetch": {},
      "cordova-plugin-zeroconf": {},
      "cordova-plugin-qr-barcode-scanner": {},
      "cordova-wheel-selector-plugin": {}
    }
  }
}
