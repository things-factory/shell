{
  "name": "@things-factory/shell",
  "version": "2.3.0-beta.20",
  "description": "things factory shell",
  "bin": {
    "things-factory": "bin/things-factory",
    "things-factory-dev": "bin/things-factory-dev",
    "things-factory-migration": "bin/things-factory-migration",
    "things-factory-stop": "bin/things-factory-stop"
  },
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "repository": "http://github.com/things-factory/shell",
  "author": "Hearty Oh <heartyoh@hatiolab.com>",
  "license": "MIT",
  "things-factory": true,
  "scripts": {
    "serve": "env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server.ts",
    "serve:dev": "npm run build:server && env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server-dev.ts",
    "serve:dev:client": "env node --inspect -r ts-node/register -r tsconfig-paths/register ./server/server-dev.ts",
    "serve:dev:server": "nodemon --config nodemon-server.json",
    "serve:dev:full": "nodemon --config nodemon-full.json",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm run clean:server && tsc",
    "build:dev:client": "webpack-cli --config ./webpack.config.dev.js",
    "build:client": "npm run clean:client && webpack-cli --mode production",
    "clean": "npm run clean:server && npm run clean:client",
    "prepublish": "npm run build:server",
    "clean:server": "rm -rf dist-server",
    "clean:client": "rm -rf dist-client",
    "start": "node dist-server/server.js",
    "migration": "npm run build:server && node bin/things-factory-migration",
    "migration:generate": "node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ./node_modules/typeorm/cli.js migration:revert",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
