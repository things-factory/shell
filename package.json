    "koa2-connect-history-api-fallback": "^0.1.2",
    "koa2-cors": "^2.0.6",
    "lit-element": "^2.1.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.19.0",
    "package-json": "^6.3.0",
    "pg": "^7.9.0",
    "promises-all": "^1.0.0",
    "puppeteer": "^1.15.0",
    "puppeteer-recorder": "^1.0.7",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sqlite3": "^4.0.4",
    "terser": "3.17.0",
    "text-loader": "^0.0.1",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.7.0",
    "typeorm": "0.2.16",
    "typescript": "^3.4.1",
    "url-loader": "^1.1.2",
    "web-animations-js": "^2.3.1",
    "webpack": "^4.29.6",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-client": "^4.1.1"
  },
  "devDependencies": {
    "@things-factory/auth-provider-jwt": "*",
    "@things-factory/auth-ui-jwt": "*",
    "@things-factory/board-provider-gql": "*",
    "@things-factory/board-ui": "*",
    "@things-factory/layout-ui": "*",
