    "@things-factory/menu-ui": "*",
    "@things-factory/more-ui": "*",
    "@things-factory/offline-ui": "*",
    "@things-factory/resource-ui-mobile": "*",
    "@things-factory/setting-ui": "*",
    "@things-factory/system-ui": "*",
    "@things-factory/tester": "*"
  }
}
